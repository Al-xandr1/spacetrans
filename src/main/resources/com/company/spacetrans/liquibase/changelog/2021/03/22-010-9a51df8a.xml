                <constraints nullable="false"/>
            </column>
            <column name="VOLUME" type="DOUBLE"/>
            <column name="ATMOSPHERE_ID" type="UUID">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
